pipeline {
    agent any

    stages {
        stage('Clone & Build') {
            steps {
                // Clone the code
                checkout scm

                // Build step (example: shell command)
                sh 'echo "Building project from branch: ${env.BRANCH_NAME}"'
            }
        }

        stage('Test') {
            steps {
                sh 'echo "Running tests..."'
            }
        }
    }
}
